******* SkyWater sky130 model library *********



* Typical corner (tt)
.lib tt
.option scale=1.0u
.param MC_MM_SWITCH=0
.param MC_PR_SWITCH=0
.param corner_factor=1
.param process_mc_factor=1
.param mismatch_factor=1

.include "../sky130_fd_pr/combined_models/continuous/parameters_fet_tt.spice"
.include "../sky130_fd_pr/combined_models/continuous/parameters_res_nom.spice"
.include "../sky130_fd_pr/combined_models/continuous/parameters_cap_nom.spice"

* Include the model files
.include "../sky130_fd_pr/combined_models/continuous/models_global.spice"
.include "../sky130_fd_pr/combined_models/continuous/models_fet.spice"
.include "../sky130_fd_pr/combined_models/continuous/models_bjt.spice"
.include "../sky130_fd_pr/combined_models/continuous/models_diodes.spice"
.include "../sky130_fd_pr/combined_models/continuous/models_resistors.spice"
.include "../sky130_fd_pr/combined_models/continuous/models_capacitors.spice"

* generic models
.include "./sky130A.spice.mdl"
.endl tt


